{
  "name": "zanimo-editor",
  "description": "the Zanimo.js editor",
  "version": "0.0.8",
  "author": "Paul Panserrieu <paul@42loops.com> (http://42loops.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peutetre/zanimo-editor.git"
  },
  "scripts": {
    "appcache": "./bin/appcache.js",
    "index:cordova": "./bin/index.js cordova",
    "index:web": "./bin/index.js web",
    "env:dev": "./bin/env.js dev",
    "env:staging": "./bin/env.js staging",
    "env:prod": "./bin/env.js prod",
    "env:cordova": "./bin/env.js cordova",
    "config:ios": "./bin/config.js com.42loops.zanimo",
    "config:android": "./bin/config.js com.fortytwoloops.zanimo",
    "browserify:debug": "./node_modules/browserify/bin/cmd.js ./lib/app.js -o ./www/app.js --debug",
    "browserify:prod": "./node_modules/browserify/bin/cmd.js ./lib/app.js -o ./www/app.js",
    "css": "rm -f ./www/css/style.css; cat ./styl/style.styl | ./node_modules/stylus/bin/stylus > ./www/css/style.css",
    "common:web": "npm run appcache && npm run index:web && npm run css",
    "common:cordova": "npm run index:cordova && npm run css && npm run env:cordova && npm run browserify:prod",
    "dev": "npm run common:web && npm run env:dev && npm run browserify:debug",
    "prod": "npm run common:web && npm run env:prod && npm run browserify:prod",
    "staging": "npm run common:web && npm run env:staging && npm run browserify:debug",
    "cordova:ios" : "cd app; ../node_modules/cordova/bin/cordova prepare ios; cd ..",
    "cordova:android" : "cd app; ../node_modules/cordova/bin/cordova prepare android; cd ..",
    "ios": "npm run common:cordova && npm run config:ios && npm run cordova:ios",
    "android": "npm run common:cordova && npm run config:android && npm run cordova:android",
    "all": "npm run ios && npm run android"
  },
  "dependencies": {
    "q": "1.0.0",
    "zanimo": "1.1.0",
    "qstart": "0.1.x",
    "mask": "0.2.2",
    "github-api": "0.7.0",
    "cookie": "0.1.0",
    "qajax": "0.2.2",
    "qimage": "1.1.1",
    "image2base64": "0.0.2",
    "mobile-button": "0.2.0",
    "qanimationframe": "2.1.2",
    "iscroll": "~5.1.1"
  },
  "devDependencies": {
    "browserify": "3.20.x",
    "stylus": "0.42.0",
    "preprocess": "~2.0.0",
    "cordova": "3.4.1-0.1.0"
  }
}
