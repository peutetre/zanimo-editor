{
  "name": "zanimo-editor",
  "description": "the Zanimo.js editor",
  "version": "0.0.2",
  "author": "Paul Panserrieu <paul@42loops.com> (http://42loops.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peutetre/zanimo-editor.git"
  },
  "scripts": {
    "p-appcache": "node -e \"require('preprocess').preprocessFileSync('./www/manifest.appcache.tmpl', './www/manifest.appcache', {D:(new Date())})\"",
    "p-ios": "node -e \"require('preprocess').preprocessFileSync('./www/index.tmpl', './www/index.html', {ZANIMO_ENV:'cordova'})\"",
    "p-web": "node -e \"require('preprocess').preprocessFileSync('./www/index.tmpl', './www/index.html', {ZANIMO_ENV:'web'})\"",
    "target-dev": "node -e \"require('preprocess').preprocessFileSync('./env/index.tmpl', './env/index.js', {TARGET:'dev'})\"",
    "target-staging": "node -e \"require('preprocess').preprocessFileSync('./env/index.tmpl', './env/index.js', {TARGET:'staging'})\"",
    "target-prod": "node -e \"require('preprocess').preprocessFileSync('./env/index.tmpl', './env/index.js', {TARGET:'prod'})\"",
    "target-ios": "node -e \"require('preprocess').preprocessFileSync('./env/index.tmpl', './env/index.js', {TARGET:'cordova'})\"",
    "css": "rm -f ./www/css/style.css; cat ./styl/style.styl | ./node_modules/stylus/bin/stylus > ./www/css/style.css",
    "dev": "npm run p-appcache; npm run target-dev; npm run css; npm run p-web; ./node_modules/browserify/bin/cmd.js ./lib/app.js -o ./www/app.js --debug",
    "prod": "npm run p-appcache; npm run target-prod; npm run css; npm run p-web; ./node_modules/browserify/bin/cmd.js ./lib/app.js -o ./www/app.js",
    "staging": "npm run p-appcache; npm run target-staging; npm run css; npm run p-web; ./node_modules/browserify/bin/cmd.js ./lib/app.js -o ./www/app.js",
    "ios": "npm run target-ios; npm run css; npm run p-ios; ./node_modules/browserify/bin/cmd.js ./lib/app.js -o ./www/app.js; cd app; cordova prepare ios; cd .."
  },
  "dependencies": {
    "q": "1.0.0",
    "zanimo": "1.1.0",
    "qstart": "0.1.x",
    "mask": "0.2.1",
    "github-api": "0.7.0",
    "cookie": "0.1.0",
    "qajax": "0.2.2",
    "qimage": "1.1.1",
    "image2base64": "0.0.2",
    "mobile-button": "0.1.2",
    "qanimationframe": "2.1.1"
  },
  "devDependencies": {
    "browserify": "3.20.x",
    "stylus": "0.42.0",
    "preprocess": "~2.0.0"
  }
}
